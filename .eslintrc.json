{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "partylich",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "flowtype/semi": ["error", "always"],
    "flowtype/require-valid-file-annotation": ["error", "always"]
  }
}
